#include <<stdio.h>

int main() {
    int numero;
    int n1,n2,n3,n4,n5


    printf("digite um numero de 5 digitos: ");
    scanf("%d", &numero);

    n1 = numero / 10000;
    n2 = (numero / 1000) % 10;
    n3 = (numero / 100) % 10;
    n4 = (numero / 10 )% 10;
    n5 = (numero / 10 )% 10;

    printf ("%d  %d  %d  %d  %d  ", n1,n2,n3,n4,n5);

    return 0;
}